  },
  "homepage": "https://github.com/wgonz94/SearchBooks#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "gh-pages": "^2.2.0",
    "mongoose": "^5.8.1",
    "nodemon": "^2.0.2",
    "save": "^2.4.0"
  }
